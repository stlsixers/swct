- content_for :title, "Card Sets"
%h1 Card Sets 
= smart_listing_controls_for(:card_sets, {class: "form-inline text-right"}) do
	.form-group.filter.input-append
		= text_field_tag :filter, '', class: "search form-control", placeholder: "Search card set...", autocomplete: "off"
		%button.btn-primary.disabled{type: "submit"}
			%span.glyphicon.glyphicon-search
%p
= smart_listing_render(:card_sets)